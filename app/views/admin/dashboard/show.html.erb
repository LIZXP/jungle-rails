<h1>Store Status</h1>

<table class="table table-striped">
  <tr>
      <th scope="col" class="info text-center status">
        <p>Product Count</p>
      </th>
      <th scope="col" class="info text-center status">
      <p>Category Count</p>
      </th>
  </tr>
  <tbody>
    <tr>
      <td class="success text-center status"><%= @product_count %></td>
      <td class="success text-center status"><%= @category_count %></td>
    </tr>
  </tbody>
</table>
